<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
			<ROOT>
				<TCEforms>
                        <sheetTitle>OS</sheetTitle>
                </TCEforms>
				<type>array</type>
				<el>
					<additionalFiles>
						<TCEforms>
							<label>Additional files pattern - allowed to exists on remotes (prefixed with typo3_src/) (format: filename[=checksum]) (one path per line)</label>
							<config>
								<type>text</type>
								<rows>5</rows>
								<cols>50</cols>
							</config>
						</TCEforms>
					</additionalFiles>
					<missingFiles>
						<TCEforms>
							<label>Missing files pattern - allowed to be missing on remotes (prefixed with typo3_src/) (one path per line)</label>
							<config>
								<type>text</type>
								<rows>5</rows>
								<cols>50</cols>
							</config>
						</TCEforms>
					</missingFiles>
				</el>
			</ROOT>

</T3DataStructure>